{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["@redwoodjs/eslint-config"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
